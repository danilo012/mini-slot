{
  "name": "slot-game-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.0",
    "sass": "^1.63.6",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-glsl": "^1.1.2",
    "vite-plugin-html": "^3.2.0",
    "vitest": "^0.33.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@pixi/assets": "^7.2.4",
    "gsap": "^3.12.2",
    "ms": "^2.1.3",
    "pixi.js": "^7.2.4"
  },
  "jest": {
    "verbose": false,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{(j|t)s}"
    ],
    "modulePathIgnorePatterns": [
      "index.ts"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[tj]s"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {},
    "coverageDirectory": "<rootDir>/test/reports",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  }
}
